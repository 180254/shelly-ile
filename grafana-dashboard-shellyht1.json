          "rawSql": "with\n    shelly_ht_meter1b as (\n        select timestamp, is_valid, tmp_is_valid\n        from 'shelly_ht_meter1'\n        where device_name = '$device_name' and $__timeFilter(timestamp)\n    ),\n    count_all as (\n        select count() as value\n        from shelly_ht_meter1b\n    ),\n    count_not_valid as (\n        select count() as value\n        from shelly_ht_meter1b\n        where is_valid = false or tmp_is_valid = false\n    )\nselect \n    cast(0 as timestamp) time,\n    ((1.0*count_not_valid.value)/count_all.value) as value\nfrom count_all\ncross join count_not_valid",
